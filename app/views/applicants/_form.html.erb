<%= form_for @applicant do |form| %>

  <%= render @applicant.user %>


  <%= form.fields_for :answers do |answer_fields| %>
    <%= render "form_answer_field", answer: answer_fields.object, f: answer_fields  %>
  <% end %>

  <div>
    <%= form.submit class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium bg-green-200" %>
  </div>
<% end %>
