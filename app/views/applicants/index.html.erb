<div>
  <h1 class="font-bold text-4xl">Applicants</h1>
  <ul>
    <% @applicants.each do |applicant| %>
      <li><%= link_to applicant.user.name, applicant, class: "py-3 px-5 inline-block font-medium" %></li>
    <% end %>
    <%= link_to "New Applicant", new_applicant_path, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
  </ul>
</div>



